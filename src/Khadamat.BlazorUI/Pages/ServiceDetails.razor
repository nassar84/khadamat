@page "/service/{Id:int}"
@using Khadamat.Application.DTOs
@using Khadamat.BlazorUI.Services
@using Khadamat.BlazorUI.Services.Auth
@using Microsoft.AspNetCore.Components.Authorization
@inject ApiClient Api
@inject IAuthService AuthService
@inject AuthenticationStateProvider AuthStateProvider
@inject NavigationManager Navigation
@inject IJSRuntime JS

<div class="service-details-page pb-5">
    @if (service == null)
    {
        <div class="d-flex justify-content-center align-items-center vh-100">
            <div class="spinner-border text-primary" role="status"></div>
        </div>
    }
    else
    {
        <!-- Hero & Main Info -->
        <div class="hero-section position-relative">
            <div class="hero-bg" style="background-image: url('@mainImage');"></div>
            <div class="hero-overlay"></div>
            
            <div class="container-fluid position-relative z-1 pt-4 px-lg-5">
                <!-- Header Actions -->
                <div class="d-flex justify-content-between align-items-start mb-4">
                    <button class="btn btn-glass-icon text-white" @onclick="GoBack">
                        <i class="fa-solid fa-arrow-right"></i>
                    </button>
                    
                    <div class="d-flex gap-2">
                        @if (CanEdit)
                        {
                            <button class="btn btn-glass-icon text-warning" @onclick='() => OpenEdit("main")'>
                                <i class="fa-solid fa-pen"></i>
                            </button>
                        }
                        <button class="btn btn-glass-icon @(service.IsFavorite ? "text-danger" : "text-white")" @onclick="ToggleFavorite">
                            <i class="@(service.IsFavorite ? "fa-solid" : "fa-regular") fa-heart"></i>
                        </button>
                    </div>
                </div>

                <!-- Main Content Row: Side-by-Side on Desktop -->
                <div class="row g-5 text-white">
                    <!-- Left/Top: Image Gallery/Main Image -->
                    <div class="col-12 col-lg-6">
                        <div class="main-image-wrapper p-2 glass-morphism rounded-5 animate-up">
                            <div class="main-image-container rounded-4 overflow-hidden shadow-lg position-relative">
                                <img src="@mainImage" class="w-100 h-100 object-fit-cover" alt="@service.Title" />
                                <div class="price-tag-floating">
                                    <span class="fs-3 fw-bold">@service.Price?.ToString("N0")</span>
                                    <span class="small opacity-75 ms-1">ج.م</span>
                                </div>
                            </div>
                            <!-- Mini Gallery Preview -->
                            <div class="mini-gallery mt-3 d-flex gap-2 overflow-auto no-scrollbar pb-1">
                                @foreach (var img in service.Images.Take(4))
                                {
                                    <div class="mini-thumb rounded-3 overflow-hidden border border-white border-opacity-25" @onclick="() => selectedImage = img">
                                        <img src="@img" class="w-100 h-100 object-fit-cover" />
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                    
                    <!-- Right/Bottom: Primary Details -->
                    <div class="col-12 col-lg-6 d-flex flex-column justify-content-center py-lg-4">
                        <div class="animate-up" style="animation-delay: 0.1s;">
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb breadcrumb-dark mb-2">
                                    <li class="breadcrumb-item"><a href="/categories">@service.MainCategoryName</a></li>
                                    @if (!string.IsNullOrEmpty(service.CategoryName))
                                    {
                                        <li class="breadcrumb-item">@service.CategoryName</li>
                                    }
                                    @if (!string.IsNullOrEmpty(service.SubCategoryName))
                                    {
                                        <li class="breadcrumb-item active">@service.SubCategoryName</li>
                                    }
                                </ol>
                            </nav>
                            
                            <h1 class="fw-bold mb-3 display-5">@service.Title</h1>
                            
                            <div class="d-flex flex-wrap align-items-center gap-4 mb-4">
                                <div class="rating-badge-large">
                                    <i class="fa-solid fa-star text-warning"></i>
                                    <span class="fw-bold ms-1">@service.Rating.ToString("0.0")</span>
                                    <span class="text-white-50 ms-2 small">(@service.RatersCount تقييم)</span>
                                </div>
                                <div class="vr bg-white opacity-25 d-none d-md-block"></div>
                                <div class="provider-info-glass" @onclick='() => Navigation.NavigateTo("/provider/profile")'>
                                    <img src="@(service.ProviderPhoto ?? $"https://ui-avatars.com/api/?name={service.ProviderName}")" class="avatar-sm" />
                                    <div class="ms-2">
                                        <div class="fw-bold small">@service.ProviderName</div>
                                        <div class="x-small opacity-75">مزود موثوق</div>
                                    </div>
                                </div>
                            </div>

                            <div class="service-stats-pill d-flex gap-4 mb-4">
                                <div class="stat-item">
                                    <i class="fa-solid fa-location-dot me-2"></i> 
                                    @if (!string.IsNullOrEmpty(service.CityName))
                                    {
                                        <span>@service.CityName</span>
                                        @if (!string.IsNullOrEmpty(service.GovernorateName))
                                        {
                                            <span class="opacity-75"> - @service.GovernorateName</span>
                                        }
                                    }
                                    else
                                    {
                                        <span>@service.Location</span>
                                    }
                                </div>
                                @if (!string.IsNullOrEmpty(service.WorkHours))
                                {
                                    <div class="stat-item">
                                        <i class="fa-solid fa-clock me-2"></i> @service.WorkHours
                                    </div>
                                }
                                else
                                {
                                    <div class="stat-item">
                                        <i class="fa-solid fa-bolt me-2"></i> استجابة سريعة
                                    </div>
                                }
                            </div>

                            <div class="action-stack d-flex gap-2 flex-wrap">
                                @if (!string.IsNullOrEmpty(service.WhatsApp))
                                {
                                    <button class="btn btn-primary-premium flex-grow-1" @onclick="ContactWhatsApp">
                                        <i class="fa-brands fa-whatsapp fs-4"></i>
                                        <span>واتساب</span>
                                    </button>
                                }
                                @if (!string.IsNullOrEmpty(service.Phone1))
                                {
                                    <button class="btn btn-glass-success flex-grow-1" @onclick="CallProvider">
                                        <i class="fa-solid fa-phone fs-4"></i>
                                        <span>اتصال</span>
                                    </button>
                                }
                                <div class="d-flex gap-2 w-100 mt-2">
                                    @if (!string.IsNullOrEmpty(service.Telegram))
                                    {
                                        <button class="btn btn-glass-icon text-info" @onclick="JoinTelegram" title="تيليجرام">
                                            <i class="fa-brands fa-telegram"></i>
                                        </button>
                                    }
                                    @if (!string.IsNullOrEmpty(service.Facebook))
                                    {
                                        <button class="btn btn-glass-icon text-primary" @onclick="VisitFacebook" title="فيسبوك">
                                            <i class="fa-brands fa-facebook"></i>
                                        </button>
                                    }
                                    <button class="btn btn-glass-secondary px-4 flex-grow-1" @onclick="ShareService" title="مشاركة">
                                        <i class="fa-solid fa-share-nodes me-2"></i> شارك الخدمة
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Content Tabs -->
        <div class="content-container mt-n4 position-relative z-2 px-3">
            <div class="glass-card p-2 mb-3 d-flex justify-content-between rounded-4 overflow-auto text-nowrap">
                <button class="btn btn-tab @(activeTab == "details" ? "active" : "")" @onclick='() => SetTab("details")'>التفاصيل</button>
                <button class="btn btn-tab @(activeTab == "gallery" ? "active" : "")" @onclick='() => SetTab("gallery")'>معرض الصور</button>
                <button class="btn btn-tab @(activeTab == "posts" ? "active" : "")" @onclick='() => SetTab("posts")'>المنشورات</button>
                <button class="btn btn-tab @(activeTab == "reviews" ? "active" : "")" @onclick='() => SetTab("reviews")'>التقييمات</button>
            </div>

            <div class="tab-content pb-5">
                @if (activeTab == "details")
                {
                    <div class="glass-card p-4 animate-up">
                        <h5 class="fw-bold mb-3 text-primary">نبذة عن الخدمة</h5>
                        <p class="text-muted leading-loose">@service.Description</p>
                        
                        <div class="mt-4">
                            <h6 class="fw-bold mb-2"><i class="fa-solid fa-location-dot text-danger me-2"></i> الموقع بالتفصيل</h6>
                            <p class="text-muted small">@service.Address</p>
                            @if (!string.IsNullOrEmpty(service.CityName) || !string.IsNullOrEmpty(service.GovernorateName))
                            {
                                <p class="text-muted x-small">
                                    @if (!string.IsNullOrEmpty(service.CityName))
                                    {
                                        <span><i class="fa-solid fa-map-pin me-1"></i> @service.CityName</span>
                                    }
                                    @if (!string.IsNullOrEmpty(service.GovernorateName))
                                    {
                                        <span class="ms-2"><i class="fa-solid fa-map-location-dot me-1"></i> @service.GovernorateName</span>
                                    }
                                </p>
                            }
                            else if (!string.IsNullOrEmpty(service.Location))
                            {
                                <p class="text-muted x-small">(@service.Location)</p>
                            }
                        </div>

                        @if (!string.IsNullOrEmpty(service.WorkDays))
                        {
                            <div class="mt-4">
                                <h6 class="fw-bold mb-2"><i class="fa-solid fa-calendar-days text-primary me-2"></i> أيام العمل</h6>
                                <p class="text-muted small">@service.WorkDays</p>
                                <p class="text-muted x-small">@service.WorkHours</p>
                            </div>
                        }

                        @if (!string.IsNullOrEmpty(service.YouTubeUrl))
                        {
                            <div class="mt-4">
                                <h6 class="fw-bold mb-3">فيديو توضيحي</h6>
                                <div class="ratio ratio-16x9 rounded-4 overflow-hidden shadow-sm">
                                    <iframe src="@GetEmbedUrl(service.YouTubeUrl)" allowfullscreen></iframe>
                                </div>
                            </div>
                        }
                    </div>
                }
                else if (activeTab == "gallery")
                {
                    <div class="animate-up">
                        @if (CanEdit)
                        {
                            <div class="mb-3 text-center">
                                <button class="btn btn-outline-primary rounded-pill btn-sm" @onclick="AddImage">
                                    <i class="fa-solid fa-plus me-1"></i> إضافة صورة
                                </button>
                            </div>
                        }
                        
                        <div class="gallery-grid">
                            @foreach (var img in service.Images)
                            {
                                <div class="gallery-item rounded-3 overflow-hidden shadow-sm position-relative group">
                                    <img src="@img" loading="lazy" @onclick="() => ViewImage(img)" />
                                    @if (CanEdit)
                                    {
                                        <button class="btn btn-danger btn-sm position-absolute top-0 end-0 m-2 opacity-0 group-hover-opacity" 
                                                @onclick:stopPropagation="true" @onclick="() => RemoveImage(img)">
                                            <i class="fa-solid fa-trash"></i>
                                        </button>
                                    }
                                </div>
                            }
                        </div>
                    </div>
                }
                else if (activeTab == "posts")
                {
                    <div class="animate-up">
                        @if (CanEdit)
                        {
                            <div class="glass-card p-3 mb-4">
                                <textarea class="form-control border-0 bg-light mb-2" rows="2" placeholder="أكتب تحديثاً جديداً للعملاء..."></textarea>
                                <div class="d-flex justify-content-between align-items-center">
                                    <button class="btn btn-sm btn-icon text-muted"><i class="fa-solid fa-image"></i></button>
                                    <button class="btn btn-primary btn-sm rounded-pill px-4">نشر</button>
                                </div>
                            </div>
                        }
                        
                        @if (service.Posts.Any())
                        {
                            @foreach (var post in service.Posts)
                            {
                                <div class="glass-card p-3 mb-3">
                                    <div class="d-flex gap-2 mb-2">
                                        <img src="@(service.ProviderPhoto ?? "https://ui-avatars.com/api/?name=P")" class="rounded-circle" width="40" height="40"/>
                                        <div>
                                            <h6 class="fw-bold mb-0 small">@service.ProviderName</h6>
                                            <span class="text-muted x-small">@post.CreatedAt.ToString("dd MMM, hh:mm tt")</span>
                                        </div>
                                    </div>
                                    <p class="mb-2 text-dark small">@post.Content</p>
                                    @if (!string.IsNullOrEmpty(post.ImageUrl))
                                    {
                                        <img src="@post.ImageUrl" class="w-100 rounded-3 mb-2" />
                                    }
                                    <div class="border-top pt-2 mt-2 d-flex gap-3">
                                        <button class="btn btn-sm text-muted p-0"><i class="fa-regular fa-heart"></i> @post.LikesCount</button>
                                        <button class="btn btn-sm text-muted p-0"><i class="fa-regular fa-comment"></i> تعليق</button>
                                    </div>
                                </div>
                            }
                        }
                        else
                        {
                            <div class="text-center py-5 text-muted">
                                <i class="fa-regular fa-newspaper fs-1 mb-3 opacity-50"></i>
                                <p>لا توجد منشورات حتى الآن</p>
                            </div>
                        }
                    </div>
                }
                else if (activeTab == "reviews")
                {
                    <div class="animate-up">
                        <div class="glass-card p-4 text-center mb-4">
                            <h1 class="fw-bold mb-0 text-primary display-4">@service.Rating.ToString("0.0")</h1>
                            <div class="text-warning mb-2">
                                @for(int i=1; i<=5; i++) {
                                    <i class="@(i <= Math.Round(service.Rating) ? "fa-solid" : "fa-regular") fa-star fs-4"></i>
                                }
                            </div>
                            <p class="text-muted small">بناءً على @service.RatersCount تقييم</p>
                            
                            <button class="btn btn-outline-primary w-100 rounded-pill mt-2" @onclick="OpenRateModal">
                                قيم الخدمة
                            </button>
                        </div>

                        @foreach (var review in service.Reviews)
                        {
                            <div class="glass-card p-3 mb-3">
                                <div class="d-flex justify-content-between mb-2">
                                    <div class="d-flex gap-2">
                                        <img src="@(review.UserAvatar ?? $"https://ui-avatars.com/api/?name={review.UserName}")" class="rounded-circle" width="35" height="35" />
                                        <div>
                                            <h6 class="fw-bold mb-0 small">@review.UserName</h6>
                                            <div class="text-warning x-small">
                                                @for(int i=1; i<=5; i++) {
                                                    <i class="@(i <= review.Rating ? "fa-solid" : "fa-regular") fa-star"></i>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                    <span class="text-muted x-small">@review.CreatedAt.ToString("dd/MM/yyyy")</span>
                                </div>
                                <p class="text-muted small mb-0">@review.Comment</p>
                            </div>
                        }
                    </div>
                }
            </div>
        </div>
    }
</div>

<!-- Simple Modals (rendered as overlays for now) -->
@if (showRateModal)
{
    <div class="modal-overlay">
        <div class="glass-card p-4 rounded-4 w-75 animate-pop">
            <h5 class="fw-bold text-center mb-3">قيم تجربتك</h5>
            <div class="d-flex justify-content-center gap-2 mb-3">
                @for (int i = 1; i <= 5; i++)
                {
                    int rating = i;
                    <i class="@(userRating >= rating ? "fa-solid text-warning" : "fa-regular text-muted") fa-star fs-2 cursor-pointer" 
                       @onclick="() => userRating = rating"></i>
                }
            </div>
            <textarea @bind="userComment" class="form-control mb-3" rows="3" placeholder="اكتب تعليقك هنا..."></textarea>
            <div class="d-flex gap-2">
                <button class="btn btn-primary flex-grow-1 rounded-pill" @onclick="SubmitRating">إرسال</button>
                <button class="btn btn-light flex-grow-1 rounded-pill" @onclick="() => showRateModal = false">إلغاء</button>
            </div>
        </div>
    </div>
}

<!-- Lightbox Modal -->
@if (!string.IsNullOrEmpty(selectedImage))
{
    <div class="lightbox-overlay animate-fade-in" @onclick="CloseLightbox">
        <button class="btn btn-glass-icon position-absolute top-0 end-0 m-4 text-white z-3" @onclick="CloseLightbox">
            <i class="fa-solid fa-xmark"></i>
        </button>
        <img src="@selectedImage" class="lightbox-img shadow-lg rounded-4" @onclick:stopPropagation="true" />
    </div>
}

<style>
    .service-details-page {
        min-height: 100vh;
        background: #f8fafc;
    }
    .hero-section {
        min-height: 400px;
        padding-bottom: 50px;
        overflow: hidden;
    }
    .hero-bg {
        position: absolute;
        top: 0; left: 0; width: 100%; height: 120%;
        background-size: cover;
        background-position: center;
        filter: blur(20px) brightness(0.6);
        z-index: 0;
        mask-image: linear-gradient(to bottom, black 50%, transparent 100%);
        -webkit-mask-image: linear-gradient(to bottom, black 50%, transparent 100%);
    }
    .hero-overlay {
        position: absolute;
        top: 0; left: 0; width: 100%; height: 100%;
        background: radial-gradient(circle at 20% 50%, rgba(99, 102, 241, 0.4), transparent 70%);
        z-index: 0;
    }
    .btn-glass-icon {
        width: 40px; height: 40px;
        border-radius: 12px;
        background: rgba(255,255,255,0.1);
        backdrop-filter: blur(10px);
        border: 1px solid rgba(255,255,255,0.2);
        display: flex; align-items: center; justify-content: center;
        transition: all 0.2s;
    }
    .btn-glass-icon:hover { transform: scale(1.05); background: rgba(255,255,255,0.2); }
    
    .main-image-container {
        aspect-ratio: 4/3;
        border: 1px solid rgba(255,255,255,0.2);
    }

    .btn-glass {
        background: rgba(255,255,255,0.1);
        backdrop-filter: blur(10px);
        border: 1px solid rgba(255,255,255,0.2);
        color: white;
    }

    .btn-tab {
        border: none;
        background: transparent;
        padding: 8px 16px;
        border-radius: 12px;
        color: var(--text-main);
        font-weight: 500;
        transition: all 0.3s;
    }
    .btn-tab.active {
        background: var(--primary-color);
        color: white;
        box-shadow: 0 4px 10px rgba(99, 102, 241, 0.3);
    }
    
    /* Masonry Gallery */
    .gallery-grid {
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(140px, 1fr));
        grid-auto-rows: 140px;
        gap: 12px;
        grid-auto-flow: dense;
    }
    .gallery-item {
        cursor: pointer;
        transition: transform 0.2s;
    }
    .gallery-item img {
        width: 100%; height: 100%; object-fit: cover;
    }
    .gallery-item:nth-child(3n+1) { grid-row: span 2; }
    .gallery-item:nth-child(4n) { grid-column: span 2; }
    .gallery-item:hover { transform: scale(1.02); z-index: 2; }
    
    .group-hover-opacity { transition: opacity 0.2s; }
    .gallery-item:hover .group-hover-opacity { opacity: 1 !important; }

    .modal-overlay {
        position: fixed; inset: 0; z-index: 2000;
        background: rgba(0,0,0,0.6);
        backdrop-filter: blur(5px);
        display: flex; align-items: center; justify-content: center;
    }

    .lightbox-overlay {
        position: fixed; inset: 0; z-index: 3000;
        background: rgba(0,0,0,0.9);
        backdrop-filter: blur(10px);
        display: flex; align-items: center; justify-content: center;
    }
    .lightbox-img {
        max-width: 90vw; max-height: 90vh;
        object-fit: contain;
    }
</style>

@code {
    [Parameter] public int Id { get; set; }
    
    private ServiceDto? service;
    private string activeTab = "details";
    private bool showRateModal = false;
    private int userRating = 0;
    private string userComment = "";
    private string mainImage => service?.Images?.FirstOrDefault() ?? "https://via.placeholder.com/600x400?text=No+Image";

    private string? selectedImage;
    private bool CanEdit = false;

    protected override async Task OnInitializedAsync()
    {
        try 
        {
            service = await Api.GetServiceByIdAsync(Id);
            
            if (service != null && !service.Reviews.Any())
            {
                PopulateDemoData();
            }

            var authState = await AuthStateProvider.GetAuthenticationStateAsync();
            var user = authState.User;
            if (user.Identity?.IsAuthenticated == true)
            {
               if (user.IsInRole("Admin")) CanEdit = true;
            }
        }
        catch(Exception ex)
        {
            Console.WriteLine(ex);
        }
    }

    private void PopulateDemoData()
    {
        if (service == null) return;
        
        service.Rating = 4.8;
        service.RatersCount = 124;
        service.IsFavorite = false;
        service.YouTubeUrl = "https://www.youtube.com/watch?v=dQw4w9WgXcQ";
        
        // New Fields Demo Data
        service.Phone1 = "01012345678";
        service.WhatsApp = "201012345678";
        service.Telegram = "https://t.me/khadamat_demo";
        service.Facebook = "https://facebook.com/khadamat";
        service.WorkDays = "السبت - الخميس";
        service.WorkHours = "9:00 ص - 10:00 م";
        service.Address = "123 شارع الثورة، مصر الجديدة، القاهرة";

        service.Images = new List<string> 
        {
            service.Images.FirstOrDefault() ?? "https://picsum.photos/800/600?random=1",
            "https://picsum.photos/400/600?random=2",
            "https://picsum.photos/600/400?random=3",
            "https://picsum.photos/400/400?random=4",
            "https://picsum.photos/800/800?random=5",
            "https://picsum.photos/600/800?random=6"
        };
        
        service.Reviews = new List<ReviewDto>
        {
            new() { UserName = "أحمد محمد", Rating = 5, Comment = "خدمة ممتازة جداً وأنصح بها!", CreatedAt = DateTime.Now.AddDays(-2) },
            new() { UserName = "سارة علي", Rating = 4, Comment = "جيدة ولكن السعر مرتفع قليلاً", CreatedAt = DateTime.Now.AddDays(-5) }
        };

        service.Posts = new List<PostDto>
        {
            new() { Content = "خصم خاص 20% لفترة محدودة بمناسبة شهر رمضان!", CreatedAt = DateTime.Now.AddHours(-5), LikesCount = 45, ImageUrl = "https://picsum.photos/600/300?random=10" }
        };
    }

    private void SetTab(string tab) => activeTab = tab;
    
    private void GoBack() => Navigation.NavigateTo("explore");

    private void ToggleFavorite()
    {
        if (service != null) service.IsFavorite = !service.IsFavorite;
    }

    private async Task ShareService()
    {
        await JS.InvokeVoidAsync("navigator.share", new { 
            title = service?.Title, 
            text = service?.Description, 
            url = Navigation.Uri 
        });
    }

    private void ContactWhatsApp()
    {
        var wp = service?.WhatsApp ?? "";
        if (string.IsNullOrEmpty(wp)) wp = "201000000000"; // Fallback
        Navigation.NavigateTo($"https://wa.me/{wp}?text=بخصوص خدمتك: {service?.Title}");
    }

    private void CallProvider()
    {
        if (!string.IsNullOrEmpty(service?.Phone1))
            Navigation.NavigateTo($"tel:{service.Phone1}");
    }

    private void JoinTelegram()
    {
        if (!string.IsNullOrEmpty(service?.Telegram))
            Navigation.NavigateTo(service.Telegram);
    }

    private void VisitFacebook()
    {
        if (!string.IsNullOrEmpty(service?.Facebook))
            Navigation.NavigateTo(service.Facebook);
    }

    private async Task SuggestToFriend()
    {
       await JS.InvokeVoidAsync("navigator.clipboard.writeText", Navigation.Uri);
       // Ideally show a toast here
    }

